{
  "buildCommand": "npm install --legacy-peer-deps && npm run build",
  "routes": [
    {
      "src": "/game/assets/(.*)",
      "dest": "/game/assets/$1"
    },
    {
      "src": "/game/index.html",
      "dest": "/game/index.html"
    },
    {
      "src": "/game",
      "dest": "/game/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
